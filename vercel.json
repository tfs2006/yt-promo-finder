{
  "version": 2,
  "builds": [
    {
      "src": "api/*.js",
      "use": "@vercel/node"
    },
    {
      "src": "public/**",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    {
      "src": "/api/analyze",
      "dest": "/api/analyze.js"
    },
    {
      "src": "/api/collab",
      "dest": "/api/collab.js"
    },
    {
      "src": "/api/growth",
      "dest": "/api/growth.js"
    },
    {
      "src": "/api/unlisted",
      "dest": "/api/unlisted.js"
    },
    {
      "src": "/api/domain",
      "dest": "/api/domain.js"
    },
    {
      "src": "/domain",
      "dest": "/public/domain.html"
    },
    {
      "src": "/unlisted",
      "dest": "/public/unlisted.html"
    },
    {
      "src": "/growth",
      "dest": "/public/growth.html"
    },
    {
      "src": "/collab",
      "dest": "/public/collab.html"
    },
    {
      "src": "/privacy",
      "dest": "/public/privacy.html"
    },
    {
      "src": "/terms",
      "dest": "/public/terms.html"
    },
    {
      "src": "/disclaimer",
      "dest": "/public/disclaimer.html"
    },
    {
      "src": "/(.*)",
      "dest": "/public/$1"
    }
  ]
}
